{
  "name": "ts-angular",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "jest",
    "test:karma": "ng test",
    "lint": "ng lint",
    "http-server": "http-server ./dist/ts-angular --silent -p 4200",
    "e2e": "testcafe 'chrome --no-sandobox' tests --app='yarn start' --app-init-delay=30000",
    "e2e:http-server": "testcafe 'chrome:headless --no-sandobox' tests --app='yarn http-server'",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "codegen": "npx openapi-generator generate -i ./codegen/swagger.json -g typescript-angular -o ./src/apis/spacex -c ./codegen/config.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.2.2",
    "@angular/cdk": "10.2.7",
    "@angular/common": "10.2.2",
    "@angular/compiler": "10.2.2",
    "@angular/core": "10.2.2",
    "@angular/flex-layout": "10.0.0-beta.32",
    "@angular/forms": "10.2.2",
    "@angular/platform-browser": "10.2.2",
    "@angular/platform-browser-dynamic": "10.2.2",
    "@angular/router": "10.2.2",
    "@angular/youtube-player": "10.2.7",
    "@clr/angular": "4.0.5",
    "@clr/icons": "4.0.5",
    "@clr/ui": "4.0.5",
    "@webcomponents/webcomponentsjs": "2.5.0",
    "rxjs": "6.6.3",
    "tslib": "2.0.3",
    "zone.js": "0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1002.0",
    "@angular/cli": "10.2.0",
    "@angular/compiler-cli": "10.2.2",
    "@angular/language-service": "10.2.2",
    "@babel/core": "7.12.3",
    "@openapitools/openapi-generator-cli": "1.0.18-5.0.0-beta2",
    "@storybook/addon-actions": "6.0.28",
    "@storybook/addon-knobs": "6.0.28",
    "@storybook/addon-links": "6.0.28",
    "@storybook/addon-notes": "5.3.21",
    "@storybook/addon-viewport": "6.0.28",
    "@storybook/addons": "6.0.28",
    "@storybook/angular": "6.0.28",
    "@storybook/cli": "6.0.28",
    "@types/jasmine": "3.6.1",
    "@types/jasminewd2": "2.0.8",
    "@types/jest": "26.0.15",
    "@types/node": "14.14.6",
    "@types/storybook__addon-knobs": "5.2.1",
    "@types/storybook__addon-viewport": "5.2.1",
    "babel-loader": "8.1.0",
    "codelyzer": "6.0.1",
    "http-server": "0.12.3",
    "husky": "4.3.0",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "5.0.2",
    "jest": "26.6.3",
    "jest-preset-angular": "8.3.2",
    "jest-zone-patch": "0.0.10",
    "karma": "5.2.3",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "lint-staged": "10.5.1",
    "markdownlint-cli": "0.24.0",
    "prettier": "2.1.2",
    "protractor": "7.0.0",
    "testcafe": "1.9.4",
    "testcafe-angular-selectors": "0.4.1",
    "ts-jest": "26.4.4",
    "ts-node": "9.0.0",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.3.0",
    "typescript": "3.9.7"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "testMatch": [
      "**/+(*.)+(spec).+(ts|js)?(x)"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/tsconfig.spec.json",
        "stringifyContentPathRegex": "\\.html$",
        "astTransformers": {
          "before": [
            "jest-preset-angular/build/InlineFilesTransformer",
            "jest-preset-angular/build/StripStylesTransformer"
          ]
        }
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
