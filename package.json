{
  "name": "ts-angular",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "jest",
    "test:karma": "ng test",
    "lint": "ng lint",
    "http-server": "http-server ./dist/ts-angular --silent -p 4200",
    "e2e": "testcafe 'chrome --no-sandobox' tests --app='yarn start' --app-init-delay=30000",
    "e2e:http-server": "testcafe 'chrome:headless --no-sandobox' tests --app='yarn http-server'",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "codegen": "npx openapi-generator-cli generate -i ./codegen/swagger.json -g typescript-angular -o ./src/apis/spacex -c ./codegen/config.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "11.0.9",
    "@angular/cdk": "11.0.4",
    "@angular/common": "11.0.9",
    "@angular/compiler": "11.0.9",
    "@angular/core": "11.0.9",
    "@angular/flex-layout": "11.0.0-beta.33",
    "@angular/forms": "11.0.9",
    "@angular/platform-browser": "11.0.9",
    "@angular/platform-browser-dynamic": "11.0.9",
    "@angular/router": "11.0.9",
    "@angular/youtube-player": "11.0.4",
    "@clr/angular": "4.0.9",
    "@clr/icons": "4.0.9",
    "@clr/ui": "4.0.9",
    "@webcomponents/webcomponentsjs": "2.5.0",
    "rxjs": "6.6.3",
    "tslib": "2.1.0",
    "zone.js": "0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.10",
    "@angular/cli": "12.2.10",
    "@angular/compiler-cli": "12.2.10",
    "@angular/language-service": "12.2.10",
    "@babel/core": "7.12.10",
    "@openapitools/openapi-generator-cli": "2.1.16",
    "@storybook/addon-actions": "6.1.11",
    "@storybook/addon-knobs": "6.1.11",
    "@storybook/addon-links": "6.1.11",
    "@storybook/addon-notes": "5.3.21",
    "@storybook/addon-viewport": "6.1.11",
    "@storybook/addons": "6.1.11",
    "@storybook/angular": "6.1.11",
    "@storybook/cli": "6.1.11",
    "@types/jasmine": "3.6.2",
    "@types/jasminewd2": "2.0.8",
    "@types/jest": "27.0.2",
    "@types/node": "14.14.20",
    "@types/storybook__addon-knobs": "5.2.1",
    "@types/storybook__addon-viewport": "5.2.1",
    "babel-loader": "8.2.2",
    "codelyzer": "6.0.1",
    "http-server": "13.0.2",
    "husky": "7.0.2",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "7.0.0",
    "jest": "27.3.0",
    "jest-preset-angular": "10.0.1",
    "jest-zone-patch": "0.0.10",
    "karma": "6.3.4",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "lint-staged": "11.2.3",
    "markdownlint-cli": "0.26.0",
    "prettier": "2.2.1",
    "protractor": "7.0.0",
    "testcafe": "1.10.1",
    "testcafe-angular-selectors": "0.4.1",
    "ts-jest": "27.0.7",
    "ts-node": "10.3.0",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.3.0",
    "typescript": "4.0.5"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "testMatch": [
      "**/+(*.)+(spec).+(ts|js)?(x)"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "<rootDir>/tsconfig.spec.json",
        "stringifyContentPathRegex": "\\.html$",
        "astTransformers": {
          "before": [
            "jest-preset-angular/build/InlineFilesTransformer",
            "jest-preset-angular/build/StripStylesTransformer"
          ]
        }
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
